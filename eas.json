{
  "cli": {
    "version": ">= 11.0.3"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
       "env": {
        "GOOGLE_WEB_CLIENT_ID": "@secret(GOOGLE_WEB_CLIENT_ID)",
        "GOOGLE_IOS_CLIENT_ID": "@secret(GOOGLE_IOS_CLIENT_ID)",
        "GOOGLE_SERVICES_JSON": "@secret(GOOGLE_SERVICES_JSON)"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "GOOGLE_WEB_CLIENT_ID": "@secret(GOOGLE_WEB_CLIENT_ID)",
        "GOOGLE_IOS_CLIENT_ID": "@secret(GOOGLE_IOS_CLIENT_ID)",
        "GOOGLE_SERVICES_JSON": "@secret(GOOGLE_SERVICES_JSON)"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
